  "repository": {
    "type": "git",
    "url": "git+https://github.com/WilliamChelman/game-theory-ts.git"
  },
  "author": "wchelman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/WilliamChelman/game-theory-ts/issues"
  },
  "homepage": "https://github.com/WilliamChelman/game-theory-ts#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^4.0.4",
    "@types/mocha": "^2.2.42",
    "chai": "^4.1.1",
    "coveralls": "^2.13.1",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.4",
    "mocha": "^3.5.0",
    "nyc": "^11.1.0",
    "prettier": "^1.6.0",
    "ts-node": "^3.3.0",
    "tslib": "^1.7.1",
    "tslint": "^5.6.0",
    "tslint-config-prettier": "^1.3.0",
    "typescript": "^2.5.1"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add"
    ]
  },
